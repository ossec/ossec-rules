<decoder name="symantec-av">
  <prematch_pcre2>^[A-Za-z0-9@_-]{12},</prematch_pcre2>
  <pcre2 offset="after_prematch">^(\d+?),\d+?,\d+?,(\S+),(.+),</pcre2>
  <order>id, system_name, extra_data</order>
  <fts>name, location, id, system_name, extra_data</fts>
</decoder>


